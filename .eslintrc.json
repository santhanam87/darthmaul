{
  "env": {
    "es2021": true,
    "node": true,
    "jest": true,
    "browser": true
  },
  "extends": ["plugin:react/recommended", "airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-tabs": 0,
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
  }
}
